{
  "name": "uxm",
  "version": "1.0.0",
  "description": "Key web performance metrics",
  "repository": "treosh/uxm",
  "author": "Aleksey Kulikov <alekseykulikov@me.com>",
  "license": "MIT",
  "source": "src/index.js",
  "module": "dist/uxm.es.js",
  "main": "dist/uxm.js",
  "umd:main": "dist/uxm.umd.js",
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "keywords": [
    "Performance",
    "Web Performance",
    "Perceived Performance",
    "First Paint",
